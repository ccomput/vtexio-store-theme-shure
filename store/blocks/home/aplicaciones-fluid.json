{
    "flex-layout.row#aplicaciones-fluid": {
      "children": [
        "flex-layout.col#podcasters",
        "flex-layout.col#musicos",
        "flex-layout.col#jornalistas",
        "flex-layout.col#youtubers",
        "flex-layout.col#gamers"
      ],
      "props": {
        "fullWidth": false,
        "blockClass": "fluid-app"
      }
    },

    "flex-layout.col#podcasters": {
      "props": {
        "id": "podcasters",
        "blockClass": "podcasters",
        "horizontalAlign": "center"
      },
      "children": [
        "rich-text#podcasters-title",
        "rich-text#podcasters-subtitle",
        "vtex.menu@2.x:menu#Podcasters"
      ]
    },
    "rich-text#podcasters-title": {
      "props": {
        "text": "## Podcasters",
        "blockClass": "aplicacionesTitle"
      }
    },
    "rich-text#podcasters-subtitle": {
      "props": {
        "text": "Crie, capture, transmita, repita.",
        "blockClass": "aplicacionesSubTitle"
      }
    },

    "flex-layout.col#jornalistas": {
      "props": {
        "id": "jornalistas",
        "blockClass": "jornalistas",
        "horizontalAlign": "center"
      },
