      "flex-layout.row#black-friday-tree"
    ]
  },

  "flex-layout.row#black-friday": {
    "props": {
      "blockClass": "banner",
      "fullWidth": "true"
    },
    "children": [
      "image#banner-buefin"
    ]
  },

  "flex-layout.row#text-black-friday": {
    "props": {
      "blockClass": "textbuenfin"
    },
    "children": [
      "rich-text#text-black-friday"
    ]
  },

  "flex-layout.row#title-black-friday": {
    "props": {
      "blockClass": "rowbuenfin"
    },
    "children": [
      "rich-text#title-black-friday"
    ]
  },

  "flex-layout.col#product-black-friday": {
    "children": [
      "rich-text#title-black-friday"
    ],
    "props": {
      "horizontalAlign": "center",
      "blockclass": "textColumn",
      "preventVerticalStretch": true
    }
  },

  "flex-layout.row#shelf-black-friday": {
    "props": {
      "blockClass": "shelfBg",
      "paddingBottom": 6
    },
    "children": ["list-context.product-list#black-friday"]
  },
  "list-context.product-list#black-friday": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#demo-products"],
    "props": {
      "collection": "178",
      "orderBy": "OrderByTopSaleDESC"
    }
  },

  "rich-text#title-black-friday": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "## Los productos Buen Fin",
      "blockClass": "title-black-friday"
    }
  },

  "rich-text#text-black-friday": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "### Buen Fin Tienda Shure Oficial: Micrófonos y Audífonos originales de Shure en condiciones imperdibles.\n Ofertas válidas desde el 10 de noviembre hasta el 16 de noviembre",
      "blockClass": "text-black-friday"
    }
  },

