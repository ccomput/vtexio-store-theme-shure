{
  "store.custom#navidad": {
    "blocks": [
      "flex-layout.row#navidad",
      "flex-layout.row#navidad-one",
      "flex-layout.row#navidad-two",
      "flex-layout.row#navidad-tree"
    ]
  },

  "flex-layout.row#navidad": {
    "props": {
      "blockClass": "banner",
      "fullWidth": "true"
    },
    "children": [
      "image#banner-buefin"
    ]
  },

  "flex-layout.row#text-navidad": {
    "props": {
      "blockClass": "textbuenfin"
    },
    "children": [
      "rich-text#text-navidad"
    ]
  },

  "flex-layout.row#title-navidad": {
    "props": {
      "blockClass": "rowbuenfin"
    },
    "children": [
      "rich-text#title-navidad"
    ]
  },

  "flex-layout.col#product-navidad": {
    "children": [
      "rich-text#title-navidad"
    ],
    "props": {
      "horizontalAlign": "center",
      "blockclass": "textColumn",
      "preventVerticalStretch": true
    }
  },

  "image#banner-navidade": {
    "props": {
      "src": "https://shuremx.vteximg.com.br/arquivos/1900x338_buen-fin.gif",
      "maxHeight": "",
      "maxWidth": "",
      "blockClass": "banner"
    }
  },
  "flex-layout.row#shelf-navidad": {
    "props": {
      "blockClass": "shelfBg",
      "paddingBottom": 6
    },
    "children": ["list-context.product-list#navidad"]
  },
  "list-context.product-list#navidad": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#demo-products"],
    "props": {
      "collection": "178",
      "orderBy": "OrderByTopSaleDESC"
    }
  },

  "rich-text#title-navidad": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "## Los productos Buen Fin",
      "blockClass": "title-buen-fin"
    }
  },

  "rich-text#text-navidad": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "### Buen Fin Tienda Shure Oficial: Micrófonos y Audífonos originales de Shure en condiciones imperdibles.\n Ofertas válidas desde el 10 de noviembre hasta el 16 de noviembre",
      "blockClass": "text-buen-fin"
    }
  },

